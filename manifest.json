{
  "manifest_version": 3,
  "name": "Darkify",
  "description": "low-effort dark mode",
  "version": "1.2",
  
  "permissions": [
    "scripting",
    "tabs",
    "activeTab",
    "storage"
  ],
  
  "background": {
    "service_worker": "service_worker.js"
  },
  
  "action": {
	"default_title": "Run"
  }
}
